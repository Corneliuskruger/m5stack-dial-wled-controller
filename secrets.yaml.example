# Copy this file to secrets.yaml and fill in your actual values
# NEVER commit secrets.yaml to git!

# WiFi credentials
wifi_ssid: "YourWiFiSSID"
wifi_password: "YourWiFiPassword"

# ESPHome API encryption key
# Generate with: esphome config m5dial.yaml
encryptionKey: "GENERATE_YOUR_KEY_HERE"

# OTA password
ota_password: "create-a-secure-password"
